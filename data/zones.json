    "oldale": {
      "name": "Pueblo Escaso",
      "description": "Pueblo Escaso es un pequeño pueblo ubicado en el corazón de la región de Hoenn. Sirve como punto central de conexión entre varias rutas importantes. El pueblo cuenta con una Pokémon Center, una Poké Mart y varios edificios residenciales. Es aquí donde los entrenadores comienzan a explorar seriamente la región.",
      "image": "images/pueblo_escaso.jpg",
      "type": "Pueblo",
      "connections": ["Ruta 101", "Ruta 102", "Ruta 103"],
      "isTown": true,
      "locationAreaId": null
    },
